{
  "Description": "myhost",
  "ScheduleIntervalSeconds": 2,
  "LogLevel": "INFO",
  "REST": {
    "RestEnabled": true,
    "HttpThreadPoolSize": 6,
    "RestListenPort": 6060,
    "RestListenAddress": "0.0.0.0",
    "PrometheusExporterListenPort": 0,
    "SSL": {
      "SSLEnabled": true,
      "SSLCertificateFile": "/opt/appmanager/ssl/server.pem",
      "SSLCertificateKeyFile": "/opt/appmanager/ssl/server-key.pem"
    }
  },
  "Security": {
    "EncryptKey": false,
    "JWTEnabled": true,
    "Roles": {
      "manage": [
        "user-add",
        "app-control",
        "app-delete",
        "app-reg",
        "passwd-change",
        "config-set",
        "config-view",
        "user-delete",
        "file-download",
        "file-upload",
        "user-list",
        "label-delete",
        "label-set",
        "label-view",
        "user-lock",
        "permission-list",
        "role-delete",
        "role-set",
        "role-view",
        "app-run-async",
        "app-run-async-output",
        "app-run-sync",
        "user-unlock",
        "app-view-all",
        "app-view",
        "app-output-view",
        "host-resource-view"
      ],
      "usermgr": [
        "user-add",
        "passwd-change",
        "user-delete",
        "user-list",
        "user-lock",
        "permission-list",
        "role-delete",
        "role-set",
        "role-view",
        "user-unlock"
      ],
      "view": [
        "config-view",
        "label-view",
        "role-view",
        "app-view-all",
        "app-view",
        "app-output-view",
        "host-resource-view"
      ]
    },
    "Users": {
      "admin": {
        "key": "Admin123",
        "locked": false,
        "roles": [
          "manage",
          "view",
          "usermgr"
        ]
      },
      "test": {
        "key": "123",
        "locked": false,
        "roles": [

        ]
      },
      "user": {
        "key": "password",
        "locked": false,
        "roles": [
          "view"
        ]
      }
    }
  },
  "Applications": [
    {
      "command": "ping www.baidu.com -w 300",
      "name": "ping",
      "user": "root",
      "cache_lines": 30
    }
  ],
  "Consul": {
    "is_master": true,
    "is_node": true,
    "url": null,
    "url_example": "http://localhost:8500",
    "datacenter": "dc1",
    "session_TTL": 30,
    "report_interval": 15,
    "schedule_interval": 10,
    "enable_consul_security": false
  },
  "Labels": {
    "os_version": "centos7.6",
    "arch": "x86_64"
  }
}
